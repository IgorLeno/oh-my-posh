{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#FFE082",
          "style": "plain",
          "template": "@{{ .UserName }} \u279c",
          "type": "session"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#7FD5EA",
          "style": "powerline",
          "template": "(\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}) ",
          "type": "go"
        },
        {
          "foreground": "#9e7eff",
          "style": "powerline",
          "template": "(\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}) ",
          "type": "python"
        },
        {
          "foreground": "#E8CC97",
          "properties": {
            "branch_icon": "",
            "display_status": true,
            "display_stash_count": true,
            "display_upstream_icon": true,
            "display_status_icon": true
          },
          "style": "plain",
          "template": "git({{ .HEAD }}{{ if .Working.Changed }} ✚{{ end }}{{ if .Staging.Changed }} ●{{ end }})",
          "type": "git"
        },
        {
          "foreground": "#FFAB91",
          "properties": {
            "always_enabled": false,
            "style": "austin",
            "threshold": 100
          },
          "style": "powerline",
          "template": "{{ .FormattedMs }}",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 4
}
